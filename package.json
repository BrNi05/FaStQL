{
  "name": "fastql",
  "version": "1.1.0",
  "main": "src/server.js",
  "bin": "src/server.js",
  "scripts": {
    "start": "npm run webpack && node src/server.js",
    "webpack": "npx webpack",
    "docker:build": "npm run webpack && docker build -t fastql:latest .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:prune": "docker system prune -a -f",
    "release:docker": "npm run webpack && node scripts/release-docker.js",
    "format": "prettier --write \"**/*.{js,json,md}\" --log-level silent",
    "lint": "eslint . --ext .js --cache",
    "unsafe": "git config --unset core.hooksPath",
    "test": "echo \"No tests specified.\" && exit 0"
  },
  "keywords": [
    "SQL",
    "sqlcl",
    "command line",
    "server",
    "node",
    "webgui",
    "terminal"
  ],
  "author": "Barni <szocsbarnabas8@gmail.com> (https://github.com/BrNi05)",
  "license": "Apache-2.0",
  "description": "An SQL CLI wrapper that turbocharges your workflow",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrNi05/FaStQL.git"
  },
  "homepage": "https://github.com/BrNi05/FaStQL",
  "bugs": {
    "url": "https://github.com/BrNi05/FaStQL/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.0",
    "monaco-editor": "^0.55.1",
    "node-pty": "^1.1.0-beta39",
    "semver": "^7.7.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "monaco-editor-webpack-plugin": "^7.1.1",
    "prettier": "^3.7.4",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
